{
  "name": "nextjs-starter",
  "description": "A Next.js starter that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "clare chang<cwuyiqing@gmail.com>",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "export": "next export",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "npm-update": "npx npm-check-updates -u"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "latest",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.3",
    "@types/node": "16.4.13",
    "@types/prettier": "2.3.2",
    "@types/react": "17.0.16",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "4.29.0",
    "@typescript-eslint/parser": "4.29.0",
    "autoprefixer": "10.3.1",
    "eslint": "7.32.0",
    "eslint-config-alloy": "4.2.0",
    "eslint-config-next": "11.0.1",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "globby": "12.0.0",
    "lint-staged": "11.1.2",
    "next-sitemap": "1.6.148",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "tailwind-scrollbar": "1.3.1",
    "tailwindcss": "2.2.7",
    "typescript": "4.3.5"
  }
}
